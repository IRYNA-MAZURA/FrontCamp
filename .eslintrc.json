{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "globals": {
        "document": false
    },
    "rules": {
        "no-shadow": 0,
        "consistent-return": 0,
        "no-restricted-globals": [
            "error",
            "fdescribe"
        ],
        "no-console": "off",
        "linebreak-style": 0,
        "import/prefer-default-export": "off"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "allowImportExportEverywhere": true
    }
}